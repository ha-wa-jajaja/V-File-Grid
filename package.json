{
  "name": "v-file-grid",
  "version": "0.0.685",
  "description": "A Google-Drive-Behavior inspired file grid component for Vue 3.",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/v-file-grid.umd.js",
  "module": "./dist/v-file-grid.es.js",
  "types": "./dist/src/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/v-file-grid.es.js",
      "require": "./dist/v-file-grid.umd.js",
      "types": "./dist/src/index.d.ts"
    },
    "./style.css": "./dist/style.css"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ha-wa-jajaja/V-File-Grid"
  },
  "license": "MIT",
  "publishConfig": {
    "@ha-wa-jajaja:registry": "https://npm.pkg.github.com"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build && vue-tsc --emitDeclarationOnly && mv dist/src dist/types",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --fix",
    "format": "prettier -write src/"
  },
  "dependencies": {
    "vue": "^3.4.21"
  },
  "peerDependencies": {
    "@vueuse/core": "^10.9.0"
  },
  "peerDependencyRules": {
    "ignoreMissing": [
      "@vueuse/core"
    ]
  },
  "devDependencies": {
    "@tsconfig/node20": "^20.1.4",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20.17.0",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vitejs/plugin-vue-jsx": "^4.0.1",
    "@vitest/eslint-plugin": "1.1.7",
    "@vue/eslint-config-prettier": "^10.0.0",
    "@vue/eslint-config-typescript": "^14.1.1",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^9.13.0",
    "eslint-plugin-vue": "^9.29.0",
    "jsdom": "^25.0.1",
    "npm-run-all2": "^7.0.1",
    "prettier": "^3.3.3",
    "sass-embedded": "^1.81.1",
    "typescript": "~5.6.0",
    "vite": "^5.4.10",
    "vite-plugin-css-injected-by-js": "^3.5.2",
    "vite-plugin-vue-devtools": "^7.5.3",
    "vitest": "^2.1.3",
    "vue-tsc": "^2.1.6"
  }
}
